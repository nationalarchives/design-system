{% extends "nationalarchives/templates/layouts/_generic.njk" %}

{% block head %}
  <meta name="robots" content="noindex,nofollow">
{% endblock %}

{% block stylesheets %}
  <link rel="stylesheet" href="/design-system/css/all.css">
  <link rel="stylesheet" href="/design-system/css/fa.css">
  <link rel="stylesheet" href="https://use.typekit.net/kaq6qqh.css">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400..700&family=Roboto+Mono:wght@400..500&display=swap">
  <link rel="stylesheet" href="/design-system/css/ie.css" media="all and (-ms-high-contrast: none), (-ms-high-contrast: active)">
  <style>
    .tna-template__body {
      min-height: 0;
    }
  </style>
{% endblock %}

{%- set theme = "light" -%}
{#% if not themeAccent %}
{%- set themeAccent = "black" -%}
{% endif %#}

{% block cookies %}{% endblock %}
{% block header %}{% endblock %}
{% block skipLink %}{% endblock %}

{% block main %}
  <main>
    {{ contents | safe }}
  </main>
{% endblock %}

{% block footer %}{% endblock %}
{% block bodyEnd %}
  <script src="/design-system/static/scripts/all.js"></script>
  <script>
    window.TNAFrontend.initAll();
  </script>
{% endblock %}
